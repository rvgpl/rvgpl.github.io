  "version": "1.0.0",
  "description": "Personal website of Ravigopal Kesari using Gatsby.js",
  "main": "index.js",
  "scripts": {
    "dev": "gatsby develop",
    "build": "gatsby build",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx --ignore-pattern public .",
    "clean": "rm -rf ./public",
    "deploy": "npm run clean && gatsby build --prefix-paths && gh-pages -d public -b master",
    "format:js": "prettier '**/*.{js,jsx}' --write"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rvgpl/gatsby-blog.git"
  },
  "keywords": [
    "gatsby"
  ],
  "author": "rvgpl",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rvgpl/gatsby-blog/issues"
  },
  "homepage": "https://github.com/rvgpl/gatsby-blog#readme",
  "dependencies": {
    "axios": "^0.17.1",
    "bluebird": "^3.5.0",
    "gatsby": "^1.9.36",
    "gatsby-link": "^1.6.16",
    "gatsby-plugin-google-analytics": "^1.0.13",
    "gatsby-plugin-google-fonts": "^0.0.3",
    "gatsby-plugin-react-helmet": "^2.0.2",
    "gatsby-plugin-sharp": "^1.6.7",
    "gatsby-plugin-styled-components": "^2.0.0-alpha.0",
