<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

 <title>Ravigopal Kesari</title>
 <link href="http://ravigopalkesari.com/atom.xml" rel="self"/>
 <link href="http://ravigopalkesari.com/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://ravigopalkesari.com/</id>
 <author>
   <name>Ravigopal Kesari</name>
   <email>hello@ravigopalkesari.com</email>
 </author>

 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://ravigopalkesari.com{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://ravigopalkesari.com{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>